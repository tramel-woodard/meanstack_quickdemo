<% include _header %>

<h1>MEAN Social App</h1>

<% users.forEach(function(user) { %>
    <div class="panel panel-default">
        <div class-"panel-heading">
            <a href="/users/<%= user.username %>">
                <%= user.name() %>
            </a>
        </div>
        <% if (user.aboutMe) { %>
            <div class="panel-body">
                <%= user.aboutMe %>
            </div>
        <% } %>
    </div>
<% }) %>

<% include _footer %>
